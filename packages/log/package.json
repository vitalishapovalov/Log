{
  "name": "@js-utilities/log",
  "version": "0.1.2",
  "description": "Utility library for javascript logging.",
  "license": "MIT",
  "author": {
    "name": "Vitalii Shapovalov",
    "email": "<shapovalov.vitali.v@gmail.com>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitalishapovalov/js-utilities.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "logging",
    "logger",
    "automated logging",
    "styled logging",
    "js log",
    "log decorator",
    "proxy logger"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c rollup.config.ts --name log",
    "test": "jest --coverage",
    "lint": "tslint -c tslint.ts 'src/**/*.ts'"
  },
  "dependencies": {
    "@js-utilities/typecheck": "file:../typecheck",
    "chalk": "2.4.2",
    "reflect-metadata": "0.1.13",
    "template-colors-web": "1.0.0",
    "xmldom": "0.1.27"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/__tests__/.*.spec.(ts|js)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "gitHead": "fe9f1938b2a0ce1f4b3a6fde5c28622179822406"
}
